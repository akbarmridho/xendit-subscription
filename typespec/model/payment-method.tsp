@doc("Represents a payment method for a Subscriptions plan")
model PaymentMethod {
    @doc("ID for payment_method_id generated by Create Payment Method API")
    payment_method_id: string;

    @doc("Indicate the order which payments methods will be attempted for a payment cycle instance.")
    @minValue(1)
    @maxValue(5)
    rank: int32;

    @doc("Type of payment method")
    type: "EWALLET" | "DIRECT_DEBIT" | "CARD";
}
